{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": ["app/index.d.ts", "app/**/*", "lib/**/*", "lib/_preload/*.d.ts", "resources/**/*"],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "types": ["electron-vite/node"],
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./*"]
    }
  }
}
